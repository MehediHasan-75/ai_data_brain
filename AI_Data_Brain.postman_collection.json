            "body": {"mode": "raw", "raw": "{\"tableId\":1,\"rowId\":1,\"newRowData\":{\"Date\":\"2024-01-31\",\"Category\":\"Food Updated\",\"Amount\":\"60.00\",\"Description\":\"Lunch Updated\"}}"},
